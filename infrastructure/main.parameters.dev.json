{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environment": {
      "value": "dev"
    },
    "location": {
      "value": "australiasoutheast"
    },
    "projectName": {
      "value": "asx-db-scraper"
    },
    "cosmosDbAccountName": {
      "value": "[concat('asx-db-scraper-dev-', uniqueString(resourceGroup().id))]"
    },
    "keyVaultName": {
      "value": "[concat('asx-db-scraper-dev-', uniqueString(resourceGroup().id), '-kv')]"
    }
  }
}
